{OVERALL_HEADER}<table width="780" border="0" align="center" cellpadding="0" cellspacing="0" class="normal">
  <tr>
    <td valign="top"><br>
      <form name="form1" method="post" action="">
        <table border="0" cellspacing="0" cellpadding="1">
          <tr>
            <td width="150">{LANG_YNAME}:</td>
            <td><input name="name" type="text" id="name" size="42" value="{NAME}" style="width:300px;"></td>
            <td> <span class="error">{NAME_ERROR}</span></td>
          </tr>
          <tr>
            <td width="150">{LANG_YEMAIL}:</td>
            <td><input name="email" type="text" id="email" size="42" value="{EMAIL}" style="width:300px;"></td>
            <td> <span class="error">{EMAIL_ERROR}</span></td>
          </tr>
          <tr>
            <td width="150">{LANG_YUSERNAME}:</td>
            <td><input name="username" type="text" id="username" size="42" value="{USERNAME}" style="width:300px;"></td>
            <td></td>
          </tr>
          <tr>
            <td width="150">{LANG_VIOLATION}:</td>
            <td><select name="violation" style="width:300px;">
                <option selected value="{LANG_POSTCONTACT}">{LANG_POSTCONTACT}</option>
                <option value="{LANG_ADVWEBSITE}">{LANG_ADVWEBSITE}</option>
                <option value="{LANG_FAKEPROJ}">{LANG_FAKEPROJ}</option>
                <option value="{LANG_ABNORMALBID}">{LANG_ABNORMALBID}</option>
                <option value="{LANG_OTHER}">{LANG_OTHER}</option>
            </select></td>
            <td></td>
          </tr>
          <tr>
            <td width="150">{LANG_USEROTHER}:</td>
            <td><input name="username2" type="text" id="username2" value="{USERNAME2}" size="42" style="width:300px;"></td>
            <td></td>
          </tr>
          <tr>
            <td width="150">{LANG_URLVIOLATION}:</td>
            <td><input name="url" type="text" id="url" size="42" value="{REDIRECT_URL}" style="width:300px;"></td>
            <td></td>
          </tr>
          <tr>
            <td width="150">&nbsp;</td>
            <td>&nbsp;</td>
            <td></td>
          </tr>
          <tr>
            <td width="150" valign="top">{LANG_VIODETAILS}:</td>
            <td><textarea name="details" cols="32" rows="6" id="details" style="width:300px;">{DETAILS}</textarea></td>
            <td valign="top"> <span class="error">{VIOL_ERROR}</span></td>
          </tr>
          <tr>
            <td width="150">&nbsp;</td>
            <td>&nbsp;</td>
            <td></td>
          </tr>
          <tr>
            <td width="150">&nbsp;</td>
            <td><input type="submit" name="Submit" value="{LANG_REPORTVIO}"></td>
            <td></td>
          </tr>
        </table>
      </form>
      <br></td>
  </tr>
</table>
{OVERALL_FOOTER}